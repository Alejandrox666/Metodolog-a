Proceso numero_correcto_o_incorrecto
	Escribir "Algoritmo de rangos";
	Escribir "Autor:Alejandro Hernández Negrete";
	Definir num como real;
	Repetir
		Escribir "Ingrese un número";
		Leer num;
	Hasta Que num >=18 y num <=100;
	Escribir "El número es correcto";
FinProceso
